{
    "name": "sebastiaanluca/laravel-helpers",
    "type": "library",
    "description": "An extensive set of Laravel-specific helpers.",
    "keywords": [
        "laravel",
        "helpers",
        "collections",
        "macros"
    ],
    "homepage": "https://github.com/sebastiaanluca/laravel-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\Helpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SebastiaanLuca\\Helpers\\Methods\\GlobalHelpersServiceProvider",
                "SebastiaanLuca\\Helpers\\Collections\\CollectionMacrosServiceProvider"
            ]
        }
    }
}
